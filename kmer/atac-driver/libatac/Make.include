# -*- makefile -*-

LIBUTL/     :=$(call MakePath,$/../../libutil/)
LIBBIO/     :=$(call MakePath,$/../../libbio/)

$/.CXX_SRCS := $/decodeMatch.C $/matchList.C $/readHeader.C
$/.CXX_LIBS := $/libatac.a

$/.CLEAN :=$/*.o $/*~ $/core

$/libatac.a: ${$/.CXX_SRCS:.C=.o}

$(eval $/%.d $/%.o:   CXXFLAGS+=-I${LIBBIO/} -I${LIBUTL/})


