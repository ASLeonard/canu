$/.all:
	cd `dirname $@` && ${MAKE} all

$/.install:
	cd `dirname $@` && ${MAKE} install

$/.clean:
	cd `dirname $@` && ${MAKE} clean

$/.real-clean:
	cd `dirname $@` && ${MAKE} real-clean
